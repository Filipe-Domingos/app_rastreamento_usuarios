<%= simple_form_for(@rastreamento) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :visitante %>
    <%= f.input :url %>
    <%= f.input :titulo %>
    <%= f.input :data_hora %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
